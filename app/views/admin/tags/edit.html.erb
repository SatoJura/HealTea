<div class="container">
  <div class="row">
    <div class="col-lg-5 text-center mx-auto">
      <h2 class="mb-3">効能名</h2>
      <p>効能名を変更することができます</p>
      <div class="form-group form-inline justify-content-center mt-4">
      <%= form_with model: @tag, url: admin_tag_path(@tag), local: true do |f| %>
        <%= f.text_field :tag_name, class:"form-control"%>
        <%= f.submit "変更を保存する", class:"btn btn-yellow px-4" %>
      <% end %>
      </div>
    </div>
  </div>
</div>
